{
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png" },    
        "name": "School Website Checker",
        "description": "A website checker to scan harmful websites and block them.",
        "homepage_url": "https://avyukt-ballari.github.io/",
    "version": "2.1",
    "declarative_net_request": {
      "rule_resources": [{
        "id": "ruleset_1",
        "enabled": true,
        "path": "blocksite.json"
      }]
    },
    "permissions": [
      "declarativeNetRequest"
    ],
    "host_permissions":[
      "*://*.sites.google.com/*",
      "*://*.s3.amazonaws.com/*",
      "*://*.unblocked.games.com/*",
      "*://*.google.com/*"
    ],
    "manifest_version": 3
  }