{
    "default_icon": "icon-128.png",
    "default_title": "Website Checker",
    "name": "School Website Checker",
    "version": "1",
    "declarative_net_request": {
      "rule_resources": [{
        "id": "ruleset_1",
        "enabled": true,
        "path": "blocksite.json"
      }]
    },
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png" },
    "permissions": [
      "declarativeNetRequest"
    ],
    "host_permissions":[
      "*://*.sites.google.com/*",
      "*://*.s3.amazonaws.com/*",
      "*://*.unblocked.games.com/*",
      "*://*.google.com/*"
    ],
    "manifest_version": 3
  }